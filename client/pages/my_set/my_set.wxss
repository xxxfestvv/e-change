.my-set-label {
  height: 54px;
  width: 100%;
  border-bottom: 1px solid #E6E6E6;
  display: flex;
}

.my-set-title {
  color: #5A5A5A;
  font-size: 14px;
  margin: 18px 15px 0px 20px;
}

.my-set-text {
  font-size: 14px;
  margin: 18px 20px 0px 0px;
}

.my-set-bottom {
  bottom: 0;
  position: fixed;
  width: 100%;
  height: 48px;
  border-top: 1px solid #E6E6E6;
}

input {
  width: 90%;
  height: 40px;
  border: 1px solid #E6E6E6;
  margin-bottom: 10px;
  margin-left: 15px;
}