.book-title-text {
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  align-content: center;
  font-size:16px; margin-bottom:5px;
}

.my-title {
  height: 180px;
  width: 100%;
  background-color: #9CB382;
  color: white;
  border-bottom: 1px solid white;
}

.my-icon {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.my-info {
  width: 24px;
  height: 24px;
  margin: 13px 0px 0px 20px;
}

.my-set {
  width: 24px;
  height: 24px;
  margin: 13px 20px 0px 0px;
}

.my-head {
  width: 100%;
  font-size: 14px;
  font-weight: bold;
  display: flex;  
  flex-direction: column;  
  align-items: center;  
}

.my-head-image{
  overflow: hidden;
  width: 64px;
  height: 64px;
  box-shadow: 0 0 12px 2px #848484;
  border-radius: 34px;
  border-width: 4px;
  border-color: white;
  border-style: solid;
  display: block;
  margin-bottom: 5px;
}

.askforinfo {
  margin-top: 3px;
}

.my-middle {
  height: 103px;
  width: 100%;
  background-color: #9CB382;
  color: white;
  border-bottom: 1px solid white;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.my-point {
  display: flex;
  color: white;
  width: 40%;
}
.my-point image{
  height: 32px;
  width: 32px;
  margin-left: 20px;
}

.my-middle-icon {
  width: 60%;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.my-middle-icon image {
  width: 24px;
  height: 24px;
  margin-left: 40px; 
}

.my-order {
  width: 50%;
}

.my-book {
  width: 50%;
}

.tab{
  float: left;
  width: 50%;
  height: 100%;
  text-align: center;
  padding: 10rpx 0; 
  background-color: #9CB382;
  font-size: 16px;
  padding-top: 16px;
  padding-bottom: 16px;
}

.topTabSwiper{
  zoom: 1;
}

.topTabSwiper:after{
  content: ""; 
  clear: both;
  display: block;
}

.tabBorer{
  color: white;
  background-color: #9CB382;
  font-weight: bold;
  height: 100%;
  padding-top: 16px;
  padding-bottom: 16px;
}

.swiper {
  width: 100%;
}

.my-tab-book {
  height: 110px;
  background-color: white;
  border-bottom: 1px solid #E6E6E6;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}

.my-tab-book image{
  width: 57px;
  height: 79px;
  margin-left: 20px;
}

.my-tab-btn {
  background-color: #EA575C;
  width: 58px;
  height: 30px;
  border-radius: 15px;
  color: white;
  font-size: 14px;
  font-weight: bold;
  float: right;
  margin-top: 40px;
}

.comment1-description{
  padding:30rpx 0;
  display:flex;
  flex-direction:row;
  justify-content:center;
}

.score-input {
  width: 90%;
  border: 1px solid #E6E6E6;
  justify-self: center;
  margin: 10px;
}

.stars{
  background: url(data:image/png;base64,);
  width: 50rpx;
  height: 50rpx;
  background-size: 50rpx 50rpx;
  margin-left: 30rpx;
  transition: 600ms;
}
.on{
  background: url(data:image/png;base64,);
  width: 50rpx;
  height: 50rpx;
  background-size: 50rpx 50rpx;
  transition: 1s;
  transform: scale(1,1)
}